{
	"name": "dataflow5",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "alterRow1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {日付} as date,",
				"          {従業員番号} as short,",
				"          {氏名} as string,",
				"          {部署コード} as string,",
				"          {部署名} as string,",
				"          {【客観的記録】始業} as string,",
				"          {【客観的記録】終業} as string,",
				"          {【客観的記録】労働時間} as string,",
				"          {【客観的記録】休憩時間} as string,",
				"          {【自己申告】始業} as string,",
				"          {【自己申告】終業} as string,",
				"          {【自己申告】労働時間} as string,",
				"          {【自己申告】休憩時間} as string,",
				"          {【自己申告】修正理由} as string,",
				"          {【乖離時間】始業（分）} as string,",
				"          {【乖離時間】終業（分）} as string,",
				"          {【乖離時間】労働時間（分）} as short,",
				"          {【所定労働日】所定内} as string,",
				"          {【所定労働日】所定外} as string,",
				"          {【所定労働日】時間外} as string,",
				"          {【所定労働日】時間外(60H超)} as string,",
				"          {【所定休日】所定内} as string,",
				"          {【所定休日】所定外} as string,",
				"          {【所定休日】時間外} as string,",
				"          {【所定休日】時間外(60H超)} as string,",
				"          {【法定休日】休日} as string,",
				"          {【時間外集計】法定休日除く} as string,",
				"          {【時間外集計】法定休日含む} as string,",
				"          {【休暇】年休} as string,",
				"          {【休暇】代休} as string,",
				"          {【その他割増】深夜} as string,",
				"          {【その他割増】深夜(法定休日)} as string,",
				"          {【控除（修正前）】遅刻} as string,",
				"          {【控除（修正前）】早退} as string,",
				"          {【控除（修正前）】欠勤} as string,",
				"          {【控除】遅刻} as string,",
				"          {【控除】早退} as string,",
				"          {【控除】欠勤} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: true,",
				"     dateFormats: ['yyyy/MM/dd']) ~> source1",
				"source1 alterRow(deleteIf(dayOfMonth({日付})<dayOfMonth(addDays(currentDate('Asia/Tokyo'),-7)))) ~> alterRow1",
				"alterRow1 derive({日付} = replace(toString({日付}), '-', '/')) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          CardID as string,",
				"          CustomerID as string,",
				"          EmployeeID as integer",
				"     ),",
				"     deletable:true,",
				"     insertable:false,",
				"     updateable:true,",
				"     upsertable:false,",
				"     keys:['_col1_'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}